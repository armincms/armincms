{
    "name": "armincms/armincms",
    "description": "A Laravel Nova tool.",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.1.0", 
        "laravel/helpers": "^1.1", 
        "laravel/nova": "^2.10",
        "cviebrock/eloquent-sluggable": "6.0.*",  
        "lavary/laravel-menu": "1.*", 
        "intervention/image": "^2.5",
        "coderello/laravel-nova-lang": "1.*",
        "davidpiesse/nova-toggle": "0.*",
        "jenssegers/agent": "^2.6",
        "kodeine/laravel-meta": "^1.0",
        "matthiasmullie/minify": "^1.3",
        "santigarcor/laratrust": "^5.2", 
        "yajra/laravel-datatables-fractal": "^1.5",
        "armincms/annisa": "~0.1", 
        "yajra/laravel-datatables-html": "^4.20",
        "optimistdigital/nova-multiselect-field" : "1.10.2",
        "rimu/formatted-number" : "^0.2", 
        "inspheric/nova-defaultable" : "^1.2", 
        "ebess/advanced-nova-media-library" : "2.*",
        "laraning/nova-time-field" : "^0.2", 
        "coderello/laravel-nova-lang" : "^1.3", 
        "pishran/nova-rtl-theme" : "^1.3", 
        "michielkempen/nova-order-field" : "^2.0", 
        "benjacho/belongs-to-many-field" : "^1.3", 
        "owenmelbz/nova-radio-field" : "0.0.*",
        "inspheric/nova-url-field": "^1.4",
        "inspheric/nova-email-field": "^1.42",
        "bissolli/nova-phone-field" : "0.0.*",
        "superlatif/nova-tag-input": "^1.1",
        "anetwork/validation": "^2.1", 
        "armincms/core" : "~1.1",
        "armincms/json" : "~0.2",
        "armincms/nova-tab" : "0.*",  
        "armincms/localization": "~0.1",
        "armincms/nova-login": "~0.1",    
        "armincms/bios": "~0.1",   
        "armincms/many-to-many": "~0.1",   
        "armincms/nova-targomaan": "~0.1",
        "armincms/nova-translation": "1.*",
        "armincms/conversion": "~0.1",
        "zareismail/nova-policy": "~0.1",
        "ghalwash/merq-laravel-nova-theme-responsive": "^1.1",
        "emilianotisato/nova-tinymce": "^1.1", 
        "infinety-es/nova-filemanager": "^2.8",
        "torann/currency": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Armincms\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Armincms\\NovaServiceProvider",
                "Armincms\\Providers\\BlueprintServiceProvider",
                "Armincms\\Providers\\EloquentServiceProvider",
                "Armincms\\ArmincmsServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
